--helper functions

function crashed(x, y)
  val=mget(x, y)
  if fget(val,1) then
    return "wall"
  else
    x = x*8
    y = y*8
    --check player
    for node in all(nodes) do
      if x == node.x and y == node.y then
        return "player"
      end
    end
    if x == c.fruit.x and y == c.fruit.y then
      return "fruit"
    else
      return "nothing"
    end
  end
end 

function get_free_space()
  while true do
    local x = 1 + flr(rnd(14))
    local y = 1 + flr(rnd(14))
    is_free = crashed(x,y) == "nothing"
    if is_free then
      return x,y
    end
  end
end

function get_spr_prop()
  local spr_prop = {}
  if c.dir == ➡️ then
    spr_prop.n = 3
    spr_prop.fx = false
    spr_prop.fy = false
  elseif c.dir== ⬅️ then
    spr_prop.n = 3
    spr_prop.fx = true
    spr_prop.fy = false
  end
end
