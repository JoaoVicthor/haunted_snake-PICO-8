--main loop
function _init()
  nodes = {}
  c = {
    score=0,
    dir=➡️,
    idir=➡️,
    s=3,
    x=4,
    y=7,
    fx=false,
    fy=false,
    state="walking",
    frame=0,
    fruit={x=0, y=0},
    fps = 0
  }
  c.fruit = new_fruit()
  add(nodes,new_node(2,7,1))
  add(nodes,new_node(3,7,2))
  add(nodes,new_node(4,7))
end

function _update()
  input()
  if c.fps < 5 then
    c.fps += 1
  else
    move()
    collision()
    anim_snake()
    c.fps = 0
  end
end

function _draw()
  cls()
  map(0,0)
  for node in all(nodes) do
    spr(node.s, node.x, node.y, 1, 1, node.fx, node.fy)
  end
  spr(c.fruit.s, c.fruit.x, c.fruit.y, 1, 1)
  print_hud()
end

