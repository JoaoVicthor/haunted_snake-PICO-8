--class def
function new_node(x,y,s)
  
  return {
    s = s or c.s + c.frame,
    x = x*8,
    y = y*8,
    fx = c.fx,
    fy = c.fy,
    dir = c.dir,
    life = #nodes + 1
  }
end

function new_fruit()
  local x,y = get_free_space()
  return {
    s = 9 + flr(rnd(4)),
    x = x*8,
    y = y*8
  }
end



