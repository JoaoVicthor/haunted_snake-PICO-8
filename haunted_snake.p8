--game logic

function move()
  eval_idir()
  if c.dir == ➡️ then
    if c.state == "walking" then
      c.s = 3
    elseif c.state == "eating" then
      c.s = 4
    end
    c.fx = false
    c.fy = false
    c.x += 1
  elseif c.dir == ⬅️ then
    if c.state == "walking" then
      c.s = 3
    elseif c.state == "eating" then
      c.s = 4
    end
    c.fx = true
    c.fy = false
    c.x -= 1
  elseif c.dir == ⬇️ then
    if c.state == "walking" then
      c.s = 19
    elseif c.state == "eating" then
      c.s = 20
    end
    c.fx = false
    c.fy = true
    c.y += 1
  elseif c.dir == ⬆️ then
    if c.state == "walking" then
      c.s = 19
    elseif c.state == "eating" then
      c.s = 20
    end
    c.fx = false
    c.fy = false
    c.y -= 1
  end
end

function eval_idir()
  if c.idir == ➡️ and c.dir != ⬅️ or
  c.idir == ⬅️ and c.dir != ➡️ or
  c.idir == ⬇️ and c.dir != ⬆️ or
  c.idir == ⬆️ and c.dir != ⬇️ then
    c.dir = c.idir
  end
end

function input()
  if btn(➡️) then
    c.idir = ➡️
  elseif btn(⬅️) then
    c.idir = ⬅️
  elseif btn(⬇️) then
    c.idir = ⬇️
  elseif btn(⬆️) then
    c.idir = ⬆️
  end
end

function collision()
  obj = crashed(c.x,c.y)
  if obj == "wall" or obj == "player" then
    _init()
  else
    if obj == "fruit" then
      eat()
    end
    update_nodes()
  end
end

function eat()
  c.score += 1
  c.state = "eating"
  c.fruit = new_fruit()
  for node in all(nodes) do
    node.life += 1
  end
  c.frame = 0
end

function update_nodes()
  if c.dir != nodes[#nodes].dir then
    nodes[#nodes].s = knee_s(nodes[#nodes].dir, c.dir)
    nodes[#nodes].dir = c.dir
  else
    nodes[#nodes].s = spr_dir(2, c.dir)
  end
  nodes[#nodes].fx = c.fx
  nodes[#nodes].fy = c.fy
  for node in all(nodes) do
    node.life -= 1
  end
  if nodes[1].life == 0 then
    deli(nodes,1)
  end
  nodes[1].s = spr_dir(1,nodes[1].dir) 
  add(nodes,new_node(c.x,c.y))
end

function spr_dir(s, dir)
  return s + 16 * dir_axis(dir)
end

function dir_axis(dir)
  if dir == ⬅️ or dir == ➡️ then
    return 0
  else
    return 1
  end
end

function knee_s(dir_a, dir_b)
  if dir_a == ➡️ and dir_b == ⬆️ or dir_a == ➡️ and dir_b == ⬇️ then
    return 36
  elseif dir_a == ⬇️ and dir_b == ⬅️ or dir_a == ⬇️ and dir_b == ➡️ then
    return 35
  elseif dir_a == ⬆️ and dir_b == ⬅️ or dir_a == ⬆️ and dir_b == ➡️ then
    return 33
  elseif dir_a == ⬅️ and dir_b == ⬆️ or dir_a == ⬅️ and dir_b == ⬇️ then
    return 34
  end
end

function anim_snake()
  if c.state == "walking" then
    if c.frame == 0 then
      c.frame = 1
    elseif c.frame == 1 then
      c.frame = 0
    end
  elseif c.state == "eating" then
    if c.frame == 3 then
      c.state = "walking"
      c.frame = 0
    else
      c.frame += 1
    end
  end  
end
